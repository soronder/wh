{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run development",
    "development": "NODE_ENV=development concurrently --kill-others \"npm run client\" \"npm run server\"",
    "production": "npm run build && NODE_ENV=production npm run server",
    "client": "npm run start --prefix client",
    "server": "node ./bin/www",
    "build": "react-scripts build"
  },
  "engines": {
    "node": "10.x"
  },
  "dependencies": {
    "concurrently": "^6.0.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "directory-tree": "^2.2.6",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "lodash.keyby": "^4.6.0",
    "lodash.uniq": "^4.5.0",
    "lodash.without": "^4.4.0",
    "mongodb": "^3.6.4",
    "morgan": "~1.9.1",
    "uuid": "^8.3.2"
  }
}
